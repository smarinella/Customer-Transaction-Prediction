    "pkl_path = \"CatboostOpt.pkl\"\n",
    "\n",
    "with open(pkl_path, \"wb\") as f:\n",
    "    # Pickle the 'catOp' model using the highest protocol available.\n",
    "    pickle.dump(catOp, f)"
   ]
  },
